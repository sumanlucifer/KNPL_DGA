<core:FragmentDefinition xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
    xmlns="sap.ui.table"
    xmlns:m="sap.m"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core">
    <TreeTable
        id="PreEstimationTable"
        selectionMode="Single"
        enableColumnReordering="false"
        expandFirstLevel="false"
       >

        <columns>

            <Column label="{i18n>Parameters}">
                <template>
                    <m:Text text="{Parameters}" wrapping="false" />
                </template>
            </Column>

            <Column label="{i18n>Type}">
                <template>
                    <m:Text text="{Type}" wrapping="false" />
                </template>
            </Column>

            <Column label="{i18n>Class}">
                <template>
                    <m:Text text="{Class}" wrapping="false" />
                </template>
            </Column>

            <Column label="{i18n>Products}">
                <template>
                    <m:Text text="{Products}" wrapping="false" />
                </template>
            </Column>
              <Column label="{i18n>System}">
                <template>
                    <m:Text text="{System}" wrapping="false" />
                </template>
            </Column>
             <Column label="{i18n>ValueAddition}">
                <template>
                    <m:Text text="{ValueAddition}" wrapping="false" />
                </template>
            </Column>
             <Column label="{i18n>Rate}">
                <template>
                    <m:Text text="{Rate}" wrapping="false" />
                </template>
            </Column>
            <Column label="{i18n>AreaSqft}">
                <template>
                    <m:Text text="{AreaSqft}" wrapping="false" />
                </template>
            </Column>
             <Column label="{i18n>RateAccToArea}">
                <template>
                    <m:Text text="{RateAccToArea}" wrapping="false" />
                </template>
            </Column>

        </columns>

    </TreeTable>

    

</core:FragmentDefinition>