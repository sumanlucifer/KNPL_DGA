<core:FragmentDefinition xmlns:unified="sap.ui.unified" xmlns:upload="sap.m.upload" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:smartForm="sap.ui.comp.smartform" xmlns:smartField="sap.ui.comp.smartfield" xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
  
        <f:SimpleForm class="addForm" editable="true" layout="ResponsiveGridLayout" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="2" columnsL="2" columnsM="2" singleContainerFullSize="false">
            <f:content>
            
                <Label text="{i18n>Zone}" />
                  <MultiComboBox  required="true" selectedKeys="{path:'oModelControl>/MultiCombo/Zone', type :'.customMulti' }" items="{ path : '/MasterZoneSet' }"  selectionChange="onMultyZoneChange" >
					<core:Item key="{Id}" text="{Id}"/>
				</MultiComboBox>
                <Label text="{i18n>Division}"/>
                <MultiComboBox editable="{= ${oModelControl>/MultiCombo/Zone}.length > 0 }" id="idDivision"  selectedKeys="{oModelControl>/MultiCombo/Division}" items="{ path : '/MasterDivisionSet'}" selectionChange="onMultyDivisionChange" >
					<core:Item key="{Id}" text="{Id}"/>
				</MultiComboBox>
                <Label text="{i18n>Depot}" />
                <MultiInput  editable="{= ${oModelControl>/MultiCombo/Division}.length > 0 }" id="multiInputDepotAdd" tokens="{oModelControl>/MultiCombo/Depot}" valueHelpRequest="onValueHelpRequestedDepot">
	                <tokens>
                        <Token editable="false" text="{oModelControl>DepotId}" key="{oModelControl>DepotId}"/>
                    </tokens>
	            </MultiInput>
                   <Label text="{i18n>PainterType}" />
                <MultiComboBox selectedKeys="{oModelControl>/MultiCombo/PainterType}" items="{path:'/MasterPainterTypeSet', filters : [ { path : 'IsArchived' , operator : 'EQ' , value1 : false } ] }">
					<core:Item key="{Id}" text="{PainterType}"/>
				</MultiComboBox>
                <Label text="{i18n>ArcheType}" />
                <MultiComboBox selectedKeys="{oModelControl>/MultiCombo/ArcheType}"  items="{path:'/MasterArcheTypeSet', filters : [ { path : 'IsArchived' , operator : 'EQ' , value1 : false } ] }">
					<core:Item key="{Id}" text="{ArcheType}"/>
				</MultiComboBox>
           
            </f:content>
        </f:SimpleForm>
   
</core:FragmentDefinition>