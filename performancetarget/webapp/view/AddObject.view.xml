                                <RadioButton text="{i18n>All}" selected="true"/>
                                <RadioButton  text="{i18n>Specific}"/>
                            </buttons>
                      </RadioButtonGroup>
               
                         
                            <MultiInput width="10rem" enabled="{titleModel>/isSpecificDepot}" editable="{= ${oModelControl>/MultiCombo/Division}.length > 0 }" id="multiInputDepotAdd" tokens="{oModelControl>/MultiCombo/Depot}" valueHelpRequest="onValueHelpRequestedDepot">
	                <tokens>
                        <Token editable="false" text="{oModelControl>DepotId}" key="{oModelControl>DepotId}"/>
                    </tokens>
	            </MultiInput>

                     </VBox>
                      <VBox >
                    <Label text="{i18n>Job} {i18n>Location}" required="true" width="14em" />
