                      </VBox>
                          <VBox class="sapUiTinyMargin">
                         <Label text="{i18n>To}"  class="" width="10em" />
                          <!-- <DatePicker placeholder="DD/MM/YYYY" width="10em"  value="{path:'oModelControl>/AddFields/ContractorVisit/ToDate'}" ></DatePicker>  -->
                           <DatePicker  placeholder="DD/MM/YYYY" class="sapUiTinyMarginEnd" valueFormat="MM/dd/YYYY" width="10em" displayFormat="MM/dd/YYYY"  change="onToDate2"  value="{path:'oModelControl>/AddFields/ContractorVisit/ToDate'}"></DatePicker>
                      </VBox>
                         <VBox class="sapUiTinyMargin">
                      <Label  text="{i18n>ContractorVisitCount} ({i18n>Daily})"  width="14em" />
                      <StepInput value="{oModelControl>/AddFields/ContractorVisit/TargetValue}" displayValuePrecision="0"  min="0" max="9999" width="14em" enabled="true" editable="true" change="onChange"/>
                      </VBox>
                     </HBox>
                       
