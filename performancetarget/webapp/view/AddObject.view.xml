                        <DatePicker minDate="{oModelView>/CurrentDate}" class="sapUiTinyMarginEnd" placeholder="DD/MM/YYYY" valueFormat="MM/dd/YYYY" displayFormat="MM/dd/YYYY" width="10em" change="onFromDate1" value="{path:'oModelControl>/AddFields/NewLead/FromDate'}"></DatePicker>
