<mvc:View xmlns:vbm="sap.ui.vbm" height="100%" controllerName="com.knpl.dga.performancetarget.controller.AddObject" xmlns:form="sap.ui.layout.form" xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" xmlns:layout="sap.ui.layout" xmlns="sap.m" xmlns:f="sap.f" xmlns:fb="sap.ui.comp.filterbar" xmlns:vm="sap.ui.comp.variants" xmlns:smartFilterBar="sap.ui.comp.smartfilterbar">
    <f:DynamicPage busy="{oModelDisplay>/PageBusy}" busyIndicatorDelay="0"	stickySubheaderProvider="iconTabBar"
			class="sapUiNoContentPadding" showFooter="true" id="dynamicPageId" headerExpanded="true">
        <f:title>
             <f:DynamicPageTitle>
                <f:heading>
                    <Title text="Set Target" id ="idAddObjectTitle"  class="sapUiSmallMarginTopBottom"/>
                </f:heading>
                <f:breadcrumbs>
                    <Breadcrumbs  currentLocationText="Set Performance Target">
                        <!-- <Link text="{i18n>MainObject2}" press="onNavToHome"/> -->
                         <Link text="{i18n>MainObject}" press="onNavToHome"/>
                    </Breadcrumbs>
                </f:breadcrumbs>
              
            </f:DynamicPageTitle>
        </f:title>
        <f:header>
           
        </f:header>

        <f:content>
        
        <form:SimpleForm class="addForm sapUiMediumMarginBeginEnd" id="idSampleSetTarget"  editable="true" layout="ResponsiveGridLayout" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="1" columnsL="3" columnsM="2" singleContainerFullSize="true">
            <form:content>
              <VBox>
                    <Label text="{i18n>Select} {i18n>DGAType}" required="true" class= "sapUiTinyMarginTopBottom" />
               	     <RadioButtonGroup columns="3" select="onRbChnageMain" selectedIndex="{oModelControl>/Rbtn/TarGrp}">
                            <buttons>
                                <RadioButton text="{i18n>DedicatedDGA}" class="sapUiMediumMarginEnd" selected="true"/>
                                <RadioButton text="{i18n>SharedDGA}"  class="sapUiMediumMarginEnd" />
                                <RadioButton text="{i18n>All}"/>
                            </buttons>
                     </RadioButtonGroup>
               
                   <VBox class="sapUiTinyMargin">
                    <HBox width="100%">
                    <VBox >
                    <Label text="{i18n>Select} {i18n>Zone}" required="true" width="14em" />
                     <RadioButtonGroup columns="2" select="onRbChnageZone" selectedIndex="{oModelControl>/Rbtn/Zone}">
                            <buttons>
                                <RadioButton text="{i18n>All}" selected="true"/>
                                <RadioButton  text="{i18n>Specific}"/>
                            </buttons>
                     </RadioButtonGroup>
               
                   
                      <MultiComboBox enabled="{titleModel>/isSpecificZone}"  id="idZone" class="sapUiTinyMarginEnd" required="true" width="10rem" selectedKeys="{path:'oModelControl>/MultiCombo/Zone', type :'.customMulti' }" items="{ path : '/MasterZones' }"  selectionChange="onMultyZoneChange" >
					<core:Item key="{Id}" text="{Id}"/>
				     </MultiComboBox>
                    </VBox>
                           
                     <VBox>
                         <Label text="{i18n>Select} {i18n>Divison}" required="true" width="14em" />
<!-- 
                         <MultiInput width="10rem" editable="{= ${oModelControl>/MultiCombo/Zone}.length > 0 }" id="multiInputDivisonAdd" tokens="{oModelControl>/MultiCombo/Divison}" valueHelpRequest="onValueHelpRequestedDivison">
	                <tokens>
                        <Token editable="false" text="{oModelControl>DivisonId}" key="{oModelControl>DivisonId}"/>
                    </tokens>
	            </MultiInput> -->
                       <RadioButtonGroup columns="2" select="onRbChnageDivison" selectedIndex="{oModelControl>/Rbtn/Division}">
                            <buttons>
                                <RadioButton text="{i18n>All}" selected="true"/>
                                <RadioButton  text="{i18n>Specific}"/>
                            </buttons>
                     </RadioButtonGroup>
               
                     
                         <MultiComboBox enabled="{titleModel>/isSpecificDivision}" editable="{= ${oModelControl>/MultiCombo/Zone}.length > 0 }" id="idDivision" width="10rem" selectedKeys="{oModelControl>/MultiCombo/Division}" items="{ path : '/MasterDivisions'}" selectionChange="onMultyDivisionChange" >
					       <core:Item key="{Id}" text="{Id}"/>
				         </MultiComboBox> 
                     </VBox>
                     <VBox>
                  <Label text="{i18n>Select} {i18n>Depot}" required="true" width="14em" />
