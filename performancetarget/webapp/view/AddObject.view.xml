                        <VBox class="sapUiTinyMargin">
                         <Label text="{i18n>From}"  class="" width="10em" />
                         <DatePicker  minDate="{oModelView>/CurrentMonth}" class="sapUiTinyMarginEnd" placeholder="MM/YYYY" change="onFromDate5" width="10em" value="{path:'oModelControl>/AddFields/BusinessGeneration/FromDate'}"  valueFormat="MM/yyyy" displayFormat="MM/yyyy"></DatePicker>
                        <!-- <DatePicker  minDate="{oModelView>/CurrentMonth}"  placeholder="MM/YYYY"  width="10em" value="{path:'oModelControl>/AddFields/BusinessGeneration/FromDate',formatOptions: {pattern:'MM/yyyy', UTC:false }}"  displayFormat="MM/yyyy"></DatePicker> -->
           
                      </VBox>
                          <VBox class="sapUiTinyMargin">
                         <Label text="{i18n>To}"  class="" width="10em" />
                           <DatePicker  placeholder="MM/YYYY" width="10em" class="sapUiTinyMarginEnd" change="onToDate5" value="{path:'oModelControl>/AddFields/BusinessGeneration/ToDate'}" displayFormat="MM/yyyy"></DatePicker> 
