                            <buttons>
                                <RadioButton text="{i18n>All}" selected="true"/>
                                <RadioButton  text="{i18n>Specific}"/>
                            </buttons>
                     </RadioButtonGroup>
                         
                            <MultiInput width="10rem" enabled="{titleModel>/isSpecificLocation}" editable="{= ${oModelControl>/MultiCombo/Depot}.length > 0 }" id="idLocation" required="true"  tokens="{oModelControl>/MultiCombo/Location}" valueHelpRequest="onValueHelpRequestedLocation">
	                <tokens>
                        <Token editable="false" text="{oModelControl>TownId}" key="{oModelControl>TownId}"/>
                    </tokens>
	            </MultiInput>
<!--                    
                      <MultiComboBox enabled="{titleModel>/isSpecificLocation}" editable="{= ${oModelControl>/MultiCombo/Depot}.length > 0 }" id="idLocation" class="sapUiTinyMarginEnd" required="true" width="10rem" selectedKeys="{path:'oModelControl>/MultiCombo/Location', type :'.customMulti' }" items="{ path : '/MasterWorkLocations' }"  >
					<core:Item key="{TownId}" text="{TownName}"/>
				     </MultiComboBox> -->
                    </VBox>
                    </HBox>
                     </VBox>
                     
                            <!-- <Label text ="{i18n>mode}" class="sapUiTinyMarginBeginEnd" required="true" />
