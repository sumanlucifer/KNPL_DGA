                        <DatePicker minDate="{oModelView>/CurrentDate}" class="sapUiTinyMarginEnd" placeholder="DD/MM/YYYY" valueFormat="MM/dd/YYYY" displayFormat="MM/dd/YYYY" width="10em" change="onFromDate" value="{path:'oModelControl>/AddFields/LeadVisit/FromDate'}"></DatePicker>
         
                      </VBox>
                      <VBox class="sapUiTinyMargin">
                         <Label text="{i18n>To}"  class="" width="10em" />
