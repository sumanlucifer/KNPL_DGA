<mvc:View controllerName="com.knpl.pragati.ContactPainter.controller.AddEditPainter" xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:uxap="sap.uxap" height="100%">
    <Page busy="{oModelView>/busy}" busyIndicatorDelay="0" showNavButton="false" showHeader="true" navButtonPress="navPressBack">
        <headerContent>
            <Bar>
                <contentLeft>
                    <Breadcrumbs currentLocationText="{i18n>AddNewPainter}">
                        <Link text="{i18n>Painters}" press="navPressBack"/>
                    </Breadcrumbs>
                </contentLeft>
            </Bar>
        </headerContent>
        <content>

            <VBox width="100%" id="oVbxSmtTbl"></VBox>
        </content>
        <footer>
            <Toolbar>
                <ToolbarSpacer/>
                <Button width="7%" text="{i18n>Cancel}" type="Ghost" press="navPressBack"/>
                 <Button width="7%" text="{i18n>Save}" type="Emphasized" press="onPressSave"/>
            </Toolbar>
        </footer>
    </Page>
</mvc:View>
