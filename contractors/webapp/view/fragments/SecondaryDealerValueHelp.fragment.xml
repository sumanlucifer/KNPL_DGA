<core:FragmentDefinition
	xmlns="sap.ui.comp.valuehelpdialog"
	xmlns:m="sap.m"
	xmlns:fb="sap.ui.comp.filterbar"
	xmlns:core="sap.ui.core">
	<ValueHelpDialog
		title="Secondary Dealers"
		ok=".onValueHelpOkPress"
		cancel=".onValueHelpCancelPress"
		afterClose=".onValueHelpAfterClose"
        afterOpen=".onValueHelpAfterOpen"
		supportRanges="false"
		key="Id"
        descriptionKey="DealerName"
       
	>
		<filterBar>
			<fb:FilterBar advancedMode="false" search="onFilterBarSearch">
				<fb:filterGroupItems>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="Id"
						label="SAP Code"
						visibleInFilterBar="true"
					><fb:control>
							<m:Input name="Id" />
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="DealerName"
						label="Dealer Name"
						visibleInFilterBar="true"			>
					<fb:control>
							<m:Input name="DealerName" />
						</fb:control>
					</fb:FilterGroupItem>
				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>
