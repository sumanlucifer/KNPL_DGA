            <f:content>
                <core:Title text="{i18n>BankingDetails}" />
                <!-- <Label text="" /> -->
                <HBox >
                    <Link text="{i18n>Cancel}" visible="{= ${oModelControl>/AddNewBank} }" press="onAddCanNewBank" />
                    <!-- <Label /> -->
                    <Link text="{i18n>AddNewBank}" visible="{= !${oModelControl>/AddNewBank} }" press="onAddNewBank" />
                </HBox>
                <Label text="{i18n>IFSCCode}" />
                <Input maxLength="11" id="IdAbIfscCode" change="onIFSCCodeChange" enabled="{oModelControl>/AddNewBank}" valueStateText="Kindly enter a valid IFSC code" required="{oModelControl>/AddNewBank}" value="{path:'oModelView>/PainterBankDetails/IfscCode',type : 'sap.ui.model.type.String', constraints : {minLength:10,maxLength:11, search:'^[a-zA-Z0-9]*$'} }" />
