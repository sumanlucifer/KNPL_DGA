<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <Dialog title="{i18n>KycDocuments}" titleAlignment="Center" contentWidth="70%" afterClose="onDialogClose">
        <content>
            <Carousel height="400px"  arrowsPlacement="PageIndicator" class="sapUiContentPadding" loop="true" visible="{=${oModelView>/PainterKycDetails/KycTypeId}!== 2}">
                <Image height="400px" decorative="false" src="{oModelControl>/KycImage/Image1}" alt="No KYC Image Found"/>
                <Image   height="400px" decorative="false" src="{oModelControl>/KycImage/Image2}" alt="No KYC Image Found"/>
              
            </Carousel>
             <Carousel height="400px" class="sapUiContentPadding" loop="true" visible="{=${oModelView>/PainterKycDetails/KycTypeId}=== 2}">
                <Image  height="400px" decorative="false"  src="{oModelControl>/KycImage/Image1}" alt="No KYC Image Found"/>
              
            </Carousel>
        </content>
        <endButton>
            <Button text="Close" press="onPressCloseDialog" />
        </endButton>
    </Dialog>
</core:FragmentDefinition>
