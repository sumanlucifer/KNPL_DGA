                <Text text="{path:'PainterBankDetails/DocumentType',formatter:'.fmtDocumentType'}" visible="{= ${oModelView>/profileEdit} !== true }"/>
                <Label text="{i18n>BankDetailsStatus}" design="Bold"/>
                <Text text="{path:'PainterBankDetails/Status',formatter:'.fmtLowerCase'}" />
                   <!--Deepanjali changes start-->
