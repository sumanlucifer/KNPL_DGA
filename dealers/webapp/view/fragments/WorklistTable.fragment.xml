<core:FragmentDefinition xmlns="sap.m"
    xmlns:smartTable="sap.ui.comp.smarttable"
    xmlns:core="sap.ui.core">
    <smartTable:SmartTable id="idWorkListTable1" header="{i18n>MainObject2} {i18n>List}" enableAutoBinding="true" initialNoDataText="{i18n>tableNoDataText}" width="100%" editable="false" entitySet="MasterDealers" beforeRebindTable="onBindTblDealerList" tableType="ResponsiveTable" showRowCount="true" useExportToExcel="false" useVariantManagement="false" useTablePersonalisation="true" demandPopin="false" requestAtLeastFields="Id" >
        <smartTable:customToolbar>
			<OverflowToolbar>
                        <ToolbarSpacer />
                        <!-- <Button icon="sap-icon://add" type="Emphasized" text="{i18n>Add} {i18n>MainObject}" press="onPressAddObject"/> -->
                    </OverflowToolbar>
		</smartTable:customToolbar>

        <Table growing="true" growingThreshold="10" growingScrollToLoad="false" noDataText="{i18n>tableNoDataText}">
            <columns>
                <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "Name", "leadingProperty": "Name", "columnIndex": 1 ,"sortProperty": "Name","filterProperty": "Name"}'/>
                    </customData>
                    <Text text="{i18n>Name}"/>
                </Column>
                <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "DealerPhoneNumber/SMSNumber", "leadingProperty": "DealerPhoneNumber/SMSNumber", "columnIndex": 3 ,"sortProperty": "DealerPhoneNumber/SMSNumber","filterProperty": "DealerPhoneNumber/SMSNumber"}'/>
                    </customData>
                    <Text text="{i18n>PhoneNumber}"/>
                </Column>
                <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "PlantCode", "leadingProperty": "PlantCode", "columnIndex": 4 ,"sortProperty": "PlantCode","filterProperty": "PlantCode"}'/>
                    </customData>
                    <Text text="{i18n>PlantCode}"/>
                </Column>
                <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "DealerSalesDetails/Depot", "leadingProperty": "DealerSalesDetails/Depot", "columnIndex": 5 ,"sortProperty": "DealerSalesDetails/Depot","filterProperty": "DealerSalesDetails/Depot"}'/>
                    </customData>
                    <Text text="{i18n>Depot}"/>
                </Column>
                <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "DealerSalesDetails/SalesGroupId", "leadingProperty": "DealerSalesDetails/SalesGroupId", "columnIndex": 6 ,"sortProperty": "DealerSalesDetails/SalesGroupId","filterProperty": "DealerSalesDetails/SalesGroupId"}'/>
                    </customData>
                    <Text text="{i18n>SalesGroupId}"/>
                </Column>
                <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "DealerSalesDetails/SalesGroup/Description", "leadingProperty": "DealerSalesDetails/SalesGroup/Description", "columnIndex": 7 ,"sortProperty": "DealerSalesDetails/SalesGroup/Description","filterProperty": "DealerSalesDetails/SalesGroup/Description"}'/>
                    </customData>
                    <Text text="{i18n>SalesGroupName}"/>
                </Column>
                <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "FiscalYear", "leadingProperty": "FiscalYear", "columnIndex": 8 ,"sortProperty": "FiscalYear","filterProperty": "FiscalYear"}'/>
                    </customData>
                    <Text text="{i18n>FiscalYear}"/>
                </Column>
                <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "CustomerCategory", "leadingProperty": "CustomerCategory", "columnIndex": 9 ,"sortProperty": "CustomerCategory","filterProperty": "CustomerCategory"}'/>
                    </customData>
                    <Text text="{i18n>CustomerCategory}"/>
                </Column>
                <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "Leads", "leadingProperty": "Leads", "columnIndex": 11}'/>
                    </customData>
                    <Text text="{i18n>TotalLeadsProvided}"/>
                </Column>
            </columns>
            <items>
                <ColumnListItem press="onListItemPress" type="Navigation">
                    <cells>
                        <Text text="{Name}"/>
                        <Text text="{DealerPhoneNumber/SMSNumber}"/>
                        <Text text="{PlantCode}"/>
                        <Text text="{DealerSalesDetails/Depot}"/>
                        <Text text="{DealerSalesDetails/SalesGroupId}"/>
                        <Text text="{DealerSalesDetails/SalesGroup/Description}"/>
                        <Text text="{FiscalYear}"/>
                        <Text text="{CustomerCategory}"/>
                        <Text text="{= ${Leads}.length }"/>
                    </cells>
                </ColumnListItem>
            </items>
        </Table>
    </smartTable:SmartTable>
</core:FragmentDefinition>
