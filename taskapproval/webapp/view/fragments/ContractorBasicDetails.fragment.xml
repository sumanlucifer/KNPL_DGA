<core:FragmentDefinition xmlns:unified="sap.ui.unified" xmlns:upload="sap.m.upload" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:smartForm="sap.ui.comp.smartform" xmlns:smartField="sap.ui.comp.smartfield" xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
    
    <f:SimpleForm id="idContractorDetails" editable="false" layout="ResponsiveGridLayout" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="3" columnsL="3" columnsM="2" singleContainerFullSize="false" >
        <f:content>
            <core:Title text="{i18n>Membership}" />
            <Label text="{i18n>MembershipId}" />
            <Text text="{contractorModel>/TargetContractor/MembershipCard}"  />
            <Label text="{i18n>DateOfJoining}" />
            <Text text="{path:'contractorModel>/TargetContractor/JoiningDate',type:'sap.ui.model.odata.type.DateTime',formatOptions:{pattern:'dd/MM/YYYY'}}"  />
            <Label text="{i18n>MobileNo}" />
            <Text text="{contractorModel>/TargetContractor/Mobile}"  />
            <Label text="{i18n>ContractorType}" />
            <Text text="{= ${contractorModel>/TargetContractor/PainterType} === null ? 'NA' : ${contractorModel>/TargetContractor/PainterType/PainterType}}"  />
            <Label text="{i18n>AgeGroup}" />
            <Text text="{= ${contractorModel>/TargetContractor/AgeGroup} === null ? 'NA' : ${contractorModel>/TargetContractor/AgeGroup/AgeGroup} }"  />
            <Label text="{i18n>PreferredLanguage}" />
            <Text text="{contractorModel>/TargetContractor/Preference/Language/Language}"  />

            <core:Title text="{i18n>PersonalDetails}" />
            <Label text="{i18n>FullName}" />
            <Text text="{contractorModel>/TargetContractor/Name}"  />
            <Label text="{i18n>Email}" />
            <Text text="{= ${contractorModel>/TargetContractor/Email} === null ? 'NA' : ${contractorModel>/TargetContractor/Email}}"  />
            <Label text="{i18n>MeritalStatus}" />
            <Text text="{= ${contractorModel>/TargetContractor/MaritalStatus} === null ? 'NA' : ${contractorModel>/TargetContractor/MaritalStatus/MaritalStatus} }"  />
            <Label text="{i18n>Religion}" />
            <Text text="{= ${contractorModel>/TargetContractor/Religion} === null ? 'NA' : ${contractorModel>/TargetContractor/Religion/Religion} }"  />
            <Label text="{i18n>HouseType}" />
            <Text text="{= ${contractorModel>/TargetContractor/HouseType} === null ? 'NA' : ${contractorModel>/TargetContractor/HouseType} }"  />

            <core:Title text="{i18n>LocationDetails}" />
            <Label text="{i18n>Pincode}" />
            <Text text="{= ${contractorModel>/TargetContractor/PainterAddress/PinCode} === null ? 'NA' : ${contractorModel>/TargetContractor/PainterAddress/PinCode}}"  />
            <Label text="{i18n>State}" />
            <Text text="{contractorModel>/TargetContractor/PainterAddress/StateDetails/State}"  />
            <Label text="{i18n>City}" />
            <Text text="{contractorModel>/TargetContractor/PainterAddress/CityDetails/City}"  />
            <Label text="{i18n>Address}" />
            <Text text="{= ${contractorModel>/TargetContractor/PainterAddress/AddressLine1} === null ? 'NA' : ${contractorModel>/TargetContractor/PainterAddress/AddressLine1AddressLine1} } {= ${contractorModel>/TargetContractor/PainterAddress/AddressLine2} === null ? '' : ${contractorModel>/TargetContractor/PainterAddress/AddressLine1AddressLine2} }"  />
            <Label text="{i18n>Zone}" />
            <Text text="{contractorModel>/TargetContractor/ZoneId}"  />
            <Label text="{i18n>Division}" />
            <Text text="{contractorModel>/TargetContractor/DivisionId}"  />
            
        </f:content>
    </f:SimpleForm>
</core:FragmentDefinition>
