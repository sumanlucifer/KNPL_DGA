<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
		<SelectDialog
		title="{i18n>Dealers}"
        growing="true"
        growingThreshold="8"
        rememberSelections="true"
        noDataText="{i18n>Message22} {DepotId}{oModelView>/DepotId}"
		class="sapUiPopupWithPadding"
		items="{path:'/MasterDealers',sorter:{path:'Name'},parameters:{select:'Id,Name'} }"
	    search="_handlePValueHelpSearch"
		confirm="_handleDealersValueHelpConfirm"
        cancel="_onDialogClose"
		multiSelect="true">
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
            title="{Name} - {Id}"
			 />
	</SelectDialog>
</core:FragmentDefinition>
