<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
		<SelectDialog
		title="{i18n>Dealers}"
        growing="true"
        growingThreshold="8"
        rememberSelections="true"
		class="sapUiPopupWithPadding"
		items="{path:'oData2>/DealerSet',sorter:{path:'DealerName'},parameters:{select:'Id,DealerName'} }"
	    search="_handlePValueHelpSearch"
		confirm="_handleDealersValueHelpConfirm"
        cancel="_onDialogClose"
		multiSelect="true">
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
            title="{oData2>DealerName} - {oData2>Id}"
			 />
	</SelectDialog>
</core:FragmentDefinition>
