<core:FragmentDefinition xmlns="sap.m" xmlns:smartTable="sap.ui.comp.smarttable" xmlns:core="sap.ui.core">
    <smartTable:SmartTable
                            id="Dealerstable"
                            editable="false"
                            beforeRebindTable="onBeforeRebindDealersTable"                    
                            tableBindingPath="DGADealers/"
                            tableType="ResponsiveTable"
                            showRowCount="true"
                            useExportToExcel="false"
                            useVariantManagement="false"
                            useTablePersonalisation="true"
                            demandPopin="true"
                            enableAutoBinding="true"
                            placeToolbarInTable="true"
                            persistencyKey="HistoryPersistenceKey"
                            initiallyVisibleFields="Id,DealerId,DGAId,Dealer/Name,Dealer/PhoneNumber,Dealer/FiscalYear,Dealer/Visits,Dealer/TotalMappedContractor,Dealer/LeadsProvided"
                             initialNoDataText="{i18n>tableNoDataText}"
                          >
        	<smartTable:customToolbar>
			<OverflowToolbar>
                      
                        <ToolbarSpacer />
                        
                        <Button icon="sap-icon://add" type="Emphasized" text="{i18n>AddNewDealer}" press="handleDealersValueHelp" />

                        
                    </OverflowToolbar>
		</smartTable:customToolbar>

        <Table growing="true" growingThreshold="10" growingScrollToLoad="false" noDataText="{i18n>tableNoDataText}" >
            <columns>
                <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "Dealer/Name", "leadingProperty": "Dealer/Name", "columnIndex": 0 ,"sortProperty": "Dealer/Name","filterProperty": "Dealer/Name"}'/>
                    </customData>
                    <Text text="{i18n>Name}"/>
                </Column>
                <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "Dealer/PhoneNumber", "leadingProperty": "Dealer/PhoneNumber", "columnIndex": 1 ,"sortProperty": "Dealer/PhoneNumber","filterProperty": "Dealer/PhoneNumber"}'/>
                    </customData>
                    <Text text="{i18n>PhoneNumber}"/>
                </Column>
               <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "Dealer/SAPCode",  "columnIndex": 2 ,"sortProperty": "Dealer/SAPCode","filterProperty": "Dealer/SAPCode"}'/>
                    </customData>
                    <Text text="{i18n>SapCode}"/>
                </Column> 
                 <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "Dealer/SAPGroupName",  "columnIndex": 3 ,"sortProperty": "Dealer/SAPGroupName","filterProperty": "Dealer/SAPGroupName"}'/>
                    </customData>
                    <Text text="{i18n>SapGroupName}"/>
                </Column>
                <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "Dealer/FiscalYear",  "columnIndex": 4 ,"sortProperty": "Dealer/FiscalYear","filterProperty": "Dealer/FiscalYear"}'/>
                    </customData>
                    <Text text="{i18n>FiscalYear}"/>
                </Column>
                 <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "Dealer/Visits",  "columnIndex": 5 ,"sortProperty": "Dealer/Visits","filterProperty": "Dealer/Visits"}'/>
                    </customData>
                    <Text text="{i18n>Visits}"/>
                </Column> 
                <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "Dealer/TotalMappedContractor",  "columnIndex": 6 ,"sortProperty": "Dealer/TotalMappedContractor","filterProperty": "Dealer/TotalMappedContractor"}'/>
                    </customData>
                    <Text text="{i18n>TotalMappedContractor}"/>
                </Column>
                <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "Dealer/LeadsProvided", "columnIndex": 7 ,"sortProperty": "Dealer/LeadsProvided","filterProperty": "Dealer/LeadsProvided"}'/>
                    </customData>
                    <Text text="{i18n>LeadsProvided}"/>
                </Column>
                <!-- <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "Id", "leadingProperty": "Id", "columnIndex": 8 ,"sortProperty": "Id","filterProperty": "Id"}'/>
                    </customData>
                    <Text text="{i18n>Id}"/>
                </Column> -->
            
               
                

            </columns>
            <items>
                <ColumnListItem>
                    <cells>
                 
                       <Text text="{Dealer/Name}" />
                       <Text text="{Dealer/PhoneNumber}"/>
                       <Text  text="{Dealer/SAPCode}" /> 
                       <Text text="{Dealer/SAPGroupName}"/>
                        <Text text="{Dealer/FiscalYear}"/>
                        <Text text="{Dealer/Visits}" /> 
                        <Text text="{Dealer/TotalMappedContractor}"/>
                        <Text text="{Dealer/LeadsProvided}"/>

                       <!-- <Text text="{Id}" /> -->
                        

                    </cells>
                </ColumnListItem>
            </items>
        </Table>
    </smartTable:SmartTable>
</core:FragmentDefinition>
