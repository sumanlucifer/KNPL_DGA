<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
		<SelectDialog
		title="Products"
        growing="true"
        growingThreshold="8"
        rememberSelections="true"
		class="sapUiPopupWithPadding"
		items="{path:'/MasterDealers',sorter:{path:'Name'},parameters:{select:'ID,Name'} }"
	    search="_handlePValueHelpSearch"
		confirm="_handleProdValueHelpConfirm"
        cancel="_handleProdValueHelpClose"
		multiSelect="true">
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
            title="{Name}"
			 />
	</SelectDialog>
</core:FragmentDefinition>
