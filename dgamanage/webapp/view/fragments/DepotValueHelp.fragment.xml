<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
		<SelectDialog
		title="{i18n>Depot}"
        growing="true"
        growingThreshold="10"
        rememberSelections="true"
        noDataText="{i18n>tableNoDataText}"
		class="sapUiPopupWithPadding"
		items="{path:'/MasterDepots',parameters:{expand:'State'} }"
	    search="_handlePValueHelpSearch"
		confirm="_handleDepotValueHelpConfirm"
        cancel="_onDialogClose"
		multiSelect="false">
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
            title="{Depot} ( {Id} )"
			 />
	</SelectDialog>
</core:FragmentDefinition>
