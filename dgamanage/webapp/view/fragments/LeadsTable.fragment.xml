<core:FragmentDefinition xmlns="sap.m" xmlns:smartTable="sap.ui.comp.smarttable" xmlns:core="sap.ui.core">
    <smartTable:SmartTable
                           
                            editable="false"
                            beforeRebindTable="onBeforeRebindHistoryTable"                    
                          
                            tableType="ResponsiveTable"
                            showRowCount="true"
                            useExportToExcel="false"
                            useVariantManagement="false"
                            useTablePersonalisation="true"
                            demandPopin="true"
                            enableAutoBinding="true"
                            placeToolbarInTable="true"
                            persistencyKey="HistoryPersistenceKey"
                            initiallyVisibleFields="ID,DealerId,DGAId"
                             initialNoDataText="{i18n>tableNoDataText}"
                          >

        <Table growing="true" growingThreshold="10" growingScrollToLoad="false">
            <columns>
                <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "ConsumerName","columnIndex":0 }'/>
                    </customData>
                    <Text text="{i18n>ConsumerName}"/>
                </Column>
                <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "Mobile","columnIndex":1 }'/>
                    </customData>
                    <Text text="{i18n>MobileNumber}"/>
                </Column>
                <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "ServiceType","columnIndex":2 }'/>
                    </customData>
                    <Text text="{i18n>ServiceType}"/>
                </Column>
                 <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "State","columnIndex":3 }'/>
                    </customData>
                    <Text text="{i18n>State}"/>
                </Column>
                <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "Zone","columnIndex":4 }'/>
                    </customData>
                    <Text text="{i18n>Zone}"/>
                </Column>
                 <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "Division","columnIndex":5 }'/>
                    </customData>
                    <Text text="{i18n>Division}"/>
                </Column>
                 <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "Depot","columnIndex":6 }'/>
                    </customData>
                    <Text text="{i18n>Depot}"/>
                </Column>
                 <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "PinCode","columnIndex":7 }'/>
                    </customData>
                    <Text text="{i18n>PinCode}"/>
                </Column>
                <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "Stage","columnIndex":8 }'/>
                    </customData>
                    <Text text="{i18n>Stage}"/>
                </Column>
                 <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "Status","columnIndex":9 }'/>
                    </customData>
                    <Text text="{i18n>Status}"/>
                </Column>
                <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "ID", "leadingProperty": "ID", "columnIndex": 8 ,"sortProperty": "ID","filterProperty": "ID"}'/>
                    </customData>
                    <Text text="{i18n>Id}"/>
                </Column>
            
               
                

            </columns>
            <items>
                <ColumnListItem>
                    <cells>
                 
                       <Text />
                       <Text />
                       <Text />
                        <Text />
                        <Text />
                        <Text />
                        <Text />
                        <Text />
                        <Text />
                         <Text />
                       <Text text="{ID}" />
                        

                    </cells>
                </ColumnListItem>
            </items>
        </Table>
    </smartTable:SmartTable>
</core:FragmentDefinition>
