<core:FragmentDefinition xmlns="sap.m" xmlns:smartTable="sap.ui.comp.smarttable" xmlns:core="sap.ui.core">
   
         <Table id="idContractorTable" growing="true" growingThreshold="10" growingScrollToLoad="false" noDataText="{i18n>tableNoDataText}" >
            <columns>

                <Column hAlign="Begin">
                    <Text text="{i18n>Name}"/>
                </Column>
              <Column hAlign="Begin">
                    <Text text="{i18n>Mobile}"/>
             </Column>
               <Column hAlign="Begin">
                    <Text text="{i18n>MembershipId}"/>
             </Column>
            <Column hAlign="Begin">
                    <Text text="{i18n>Points}"/>
             </Column>
              <Column hAlign="Begin">
                    <Text text="{i18n>Category}"/>
             </Column>
            <Column hAlign="Begin">
                    <Text text="{i18n>DealerName}"/>
             </Column>
             <Column hAlign="Begin">
                    <Text text="{i18n>DealerCode}"/>
             </Column> 
                <Column hAlign="Begin">
                    <Text text="{i18n>KycStatus}"/>
             </Column> 
               <Column hAlign="Begin">
                    <Text text="{i18n>BankDetailsStatus}"/>
             </Column> 

            </columns>
            <dependents >
                <ColumnListItem id="oColumListItemContrators">
                    <cells>
                 
                       <!-- <Text text="{Contractor/Name}" /> -->
                      
                       <Text text="{ContractorName}" />
                        <Text text="{Contractor/Mobile}" />
                        <Text text="{Contractor/MembershipCard}" />
                         <Text text="{Contractor/RewardPoints}" />
                         <Text text="{Rating}" />
                          <Text text="{Dealer/Name}" />
                            <Text text="{Dealer/Id}" />
                                <Text text="{path:'Contractor/KYCStatus',formatter:'.formatter.fmtLowerCase'}" />
                                  <Text text="{Contractor/BankDetailsStatus, formatter:'.formatter.fmtLowerCase'}" />
                    </cells>
                </ColumnListItem>
            </dependents >
            <items>
            </items>
        </Table>
   
</core:FragmentDefinition>
