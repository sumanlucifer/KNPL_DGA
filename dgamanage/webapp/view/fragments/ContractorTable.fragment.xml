<core:FragmentDefinition xmlns="sap.m" xmlns:smartTable="sap.ui.comp.smarttable" xmlns:core="sap.ui.core">
    <smartTable:SmartTable
                            id="idContractorTable"
                            editable="false"
                            beforeRebindTable="onBeforeBindContractorTbl"                    
                            tableBindingPath="DGAContractors/"
                            tableType="ResponsiveTable"
                            showRowCount="true"
                            useExportToExcel="false"
                            useVariantManagement="false"
                            useTablePersonalisation="true"
                            demandPopin="true"
                            enableAutoBinding="true"
                            placeToolbarInTable="true"
                            persistencyKey="HistoryPersistenceKey"
                            initiallyVisibleFields="Id,Contractor/Name"
                             initialNoDataText="{i18n>tableNoDataText}"
                          >

         <Table growing="true" growingThreshold="10" growingScrollToLoad="false" noDataText="{i18n>tableNoDataText}" >
            <columns>
                <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "Contractor/Name", "leadingProperty": "Contractor/Name", "columnIndex": 0 ,"sortProperty": "Contractor/Name","filterProperty": "Contractor/Name"}'/>
                    </customData>
                    <Text text="{i18n>Name}"/>
                </Column>
                <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "PhoneNumber","columnIndex":1 }'/>
                    </customData>
                    <Text text="{i18n>PhoneNumber}"/>
                </Column>
                <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "NppId","columnIndex":2 }'/>
                    </customData>
                    <Text text="{i18n>NppId}"/>
                </Column>
                 <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "Points","columnIndex":3 }'/>
                    </customData>
                    <Text text="{i18n>Points}"/>
                </Column>
                <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "Category","columnIndex":4 }'/>
                    </customData>
                    <Text text="{i18n>Category}"/>
                </Column>
                 <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "DealerName","columnIndex":5 }'/>
                    </customData>
                    <Text text="{i18n>DealerName}"/>
                </Column>
                 <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "DealerCode","columnIndex":6 }'/>
                    </customData>
                    <Text text="{i18n>DealerCode}"/>
                </Column>
                 <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "KycStatus","columnIndex":7 }'/>
                    </customData>
                    <Text text="{i18n>KycStatus}"/>
                </Column>
                <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "BankStatus","columnIndex":8 }'/>
                    </customData>
                    <Text text="{i18n>BankDetailsStatus}"/>
                </Column>
                <!-- <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "Id", "leadingProperty": "Id", "columnIndex": 9 ,"sortProperty": "Id","filterProperty": "Id"}'/>
                    </customData>
                    <Text text="{i18n>Id}"/>
                </Column> -->
            
               
                

            </columns>
            <items>
                <ColumnListItem>
                    <cells>
                 
                       <Text text="{Contractor/Name}" />
                       <Text />
                       <Text />
                        <Text />
                        <Text />
                        <Text />
                        <Text />
                        <Text />
                        <Text />
                       <!-- <Text text="{ID}" /> -->
                        

                    </cells>
                </ColumnListItem>
            </items>
        </Table>
    </smartTable:SmartTable>
</core:FragmentDefinition>
