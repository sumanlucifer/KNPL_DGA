{
    "_version": "1.12.0",
    "sap.app": {
        "id": "com.knpl.dga.complains",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "resources": "resources.json",
        "dataSources": {
            "mainService": {
                "uri": "/api/v2/odata.svc/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/metadata.xml"
                }
            }
        },
        "crossNavigation": {
            "inbounds": {
                "com.knpl.dga.complains-inbounds": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "Manage",
                    "action": "CM",
                    "title": "{{flpTitle}}",
                    "icon": "sap-icon://quality-issue",
                    "indicatorDataSource": {
                        "dataSource": "KNPL_DGA_API",
                        "path": "/dynamic_dest/KNPL_DGA_API/api/v2/odata.svc/Complaints/$count?$filter=IsArchived eq false",
                        "refresh": 0
                    }
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://task",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "rootView": {
            "viewName": "com.knpl.dga.complains.view.App",
            "type": "XML",
            "async": true,
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.98.0",
            "libs": {
				"sap.ui.unified": {},
                "sap.ui.core": {},
                "sap.m": {},
                "sap.f": {},
                "sap.ushell": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.knpl.dga.complains.i18n.i18n"
                }
            },
            "LoginInfo": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": true,
                    "defaultUpdateMethod": "Put",
                    "useBatch": false,
                    "preliminaryContext" : true
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "com.knpl.dga.complains.view",
                "controlId": "app",
                "controlAggregation": "pages",
                "bypassed": {
                    "target": [
                        "notFound"
                    ]
                },
                "async": true
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "worklist",
                    "target": [
                        "worklist"
                    ]
                },
                {
                    "pattern": "ComplaintSet/{objectId}",
                    "name": "object",
                    "target": [
                        "object"
                    ]
                },
                {
                    "pattern": "Add/{Id}",
                    "name": "RouteAddCmp",
                    "target": [
                        "TargetAddCmp"
                    ]
                },
                {
                    "pattern": "Edit/{prop}",
                    "name": "RouteEditCmp",
                    "target": [
                        "TargetEditCmp"
                    ]
                }
            ],
            "targets": {
                "worklist": {
                    "viewName": "Worklist",
                    "viewId": "worklist",
                    "viewLevel": 1,
                    "title": "{i18n>worklistViewTitle}"
                },
                "object": {
                    "viewName": "Object",
                    "viewId": "object",
                    "viewLevel": 2,
                    "title": "{i18n>objectViewTitle}"
                },
                "TargetAddCmp": {
                    "viewName": "AddComplaint",
                    "viewId": "AddComplaint",
                    "viewLevel": 2,
                    "title": "{i18n>worklistViewTitle}"
                },
                "TargetEditCmp": {
                    "viewName": "EditComplaint",
                    "viewId": "EditComplaint",
                    "viewLevel": 2,
                    "title": "{i18n>worklistViewTitle}"
                },
                "objectNotFound": {
                    "viewName": "ObjectNotFound",
                    "viewId": "objectNotFound"
                },
                "notFound": {
                    "viewName": "NotFound",
                    "viewId": "notFound"
                }
            }
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "knpl.dga"
    }
}