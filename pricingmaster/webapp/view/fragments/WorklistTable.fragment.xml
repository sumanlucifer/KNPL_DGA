<core:FragmentDefinition xmlns="sap.m"
    xmlns:smartTable="sap.ui.comp.smarttable"
    xmlns:core="sap.ui.core">
    <smartTable:SmartTable id="idWorkListTable1" header="{i18n>MainObject}" enableAutoBinding="true" initialNoDataText="{i18n>tableNoDataText}" width="100%" editable="false" entitySet="MasterPricings" beforeRebindTable="onBindTblPriceList" tableType="ResponsiveTable" showRowCount="true" useExportToExcel="false" useVariantManagement="false" useTablePersonalisation="true" demandPopin="false" requestAtLeastFields="Id" >
        <smartTable:customToolbar>
			<!-- <OverflowToolbar> -->
                        <!-- <ToolbarSpacer /> -->
                        <!-- <Button icon="sap-icon://add" type="Emphasized" text="{i18n>Add} {i18n>MainObject}" press="onPressAddObject"/> -->
                    <!-- </OverflowToolbar>  -->
		</smartTable:customToolbar>

        <Table growing="true" growingThreshold="20" growingScrollToLoad="false" noDataText="{i18n>tableNoDataText}">
           
            <columns>
                <Column hAlign="Begin" >
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "ProductCategory/Name", "leadingProperty": "ProductCategory/Name", "columnIndex": 0 ,"sortProperty": "ProductCategory/Name","filterProperty": "ProductCategory/Name"}'/>
                    </customData>
                    <Text text="{i18n>Category}"/>
                </Column>
               
                <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "Class/Name", "leadingProperty": "Class/Name", "columnIndex": 1 ,"sortProperty": "Class/Name","filterProperty": "Class/Name"}'/>
                    </customData>
                    <Text text="{i18n>Class}"/>
                </Column>

                <!-- <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "Zone", "leadingProperty": "Zone", "columnIndex": 2 ,"sortProperty": "Zone","filterProperty": "Zone"}'/>
                    </customData>
                    <Text text="{i18n>Zone}"/>
                </Column>  -->

                <!-- <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "Division", "leadingProperty": "Division", "columnIndex": 3 ,"sortProperty": "Division","filterProperty": "Division"}'/>
                    </customData>
                    <Text text="{i18n>Division}"/>
                </Column> -->

                <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "Depot/Depot,DepotCode", "leadingProperty": "Depot/Depot,DepotCode", "columnIndex": 4 ,"sortProperty": "Depot/Depot,DepotCode","filterProperty": "Depot/Depot,DepotCode"}'/>
                    </customData>
                    <Text text="{i18n>Depot}"/>
                </Column>  

                <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "Putty", "leadingProperty": "Putty", "columnIndex": 5 ,"sortProperty": "Putty","filterProperty": "Putty"}'/>
                    </customData>
                    <Text text="{i18n>Putty}"/>
                </Column>

                  <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "Primer", "leadingProperty": "Primer", "columnIndex": 6 ,"sortProperty": "Primer","filterProperty": "Primer"}'/>
                    </customData>
                    <Text text="{i18n>Primer}"/>
                 </Column>

                   <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "Product/ProductName", "leadingProperty": "Product/ProductName", "columnIndex": 7 ,"sortProperty": "Product/ProductName","filterProperty": "Product/ProductName"}'/>
                    </customData>
                    <Text text="{i18n>Product}"/>
                </Column>
                  <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "PaintType/Name", "leadingProperty": "PaintType/Name", "columnIndex": 8 ,"sortProperty": "PaintType/Name","filterProperty": "PaintType/Name"}'/>
                    </customData>
                    <Text text="{i18n>PaintingType}"/>
                </Column>

                  <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "System/Name", "leadingProperty": "System/Name", "columnIndex": 9 ,"sortProperty": "System/Name","filterProperty": "System/Name"}'/>
                    </customData>
                    <Text text="{i18n>System}"/>
                </Column>
                  <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "Plan/ServiceSubType", "leadingProperty": "Plan/ServiceSubType", "columnIndex": 10 ,"sortProperty": "Plan/ServiceSubType","filterProperty": "Plan/ServiceSubType"}'/>
                    </customData>
                    <Text text="{i18n>Plan}"/>
                </Column>
                  <Column hAlign="Begin">
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "ValueAddition/Name", "leadingProperty": "ValueAddition/Name", "columnIndex": 11 ,"sortProperty": "ValueAddition/Name","filterProperty": "ValueAddition/Name"}'/>
                    </customData>
                    <Text text="{i18n>Value} {i18n>Addition}"/>
                </Column>
                  <Column hAlign="Begin" >
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "ContractorRate", "leadingProperty": "ContractorRate", "columnIndex": 12 ,"sortProperty": "ContractorRate","filterProperty": "ContractorRate"}'/>
                    </customData>
                    <Text text="{i18n>Contractor} {i18n>DGA} ({i18n>squarefit})"/>
                </Column>

                  <Column hAlign="Begin" >
                    <customData>
                        <core:CustomData key="p13nData" value='\{"columnKey": "DealerRate", "leadingProperty": "DealerRate", "columnIndex": 13 ,"sortProperty": "DealerRate","filterProperty": "DealerRate"}'/>
                    </customData>
                    <Text text="{i18n>Dealer} {i18n>DGA} ({i18n>squarefit})"/>
                </Column>  
                
              

            </columns>
            <items>
                <ColumnListItem >
                    <cells>
                        <Text text="{ProductCategory/Name}"/>
                        <Text text="{Class/Name}"/>
                         <!-- <Text text="{Zone}"/>  -->
                         <!-- <Text text="{Division}"/>  -->
                        <Text text="{Depot/Depot}({DepotCode})"/>
                        <Text text="{Putty}"/>
                        <Text text="{Primer}"/>
                        <Text text="{Product/ProductName}"/>
                        <Text text="{PaintType/Name}"/>
                        <Text text="{System/Name}"/>
                        <Text text="{Plan/ServiceSubType}"/>
                        <Text text="{ValueAddition/Name}"/>
                        <Text text="{ContractorRate}"/>
                        <Text text="{DealerRate}"/>

                      
                    </cells>
                </ColumnListItem>
            </items>
        </Table>
    </smartTable:SmartTable>
</core:FragmentDefinition>
