{
    "xsappname": "knpl-dga-web",
    "tenant-mode": "dedicated",
    "description": "Security profile of called application",
    "oauth2-configuration": {
        "token-validity": 604800,
        "refresh-token-validity": 2592000,
        "redirect-uris": [
            "https://*.ap11.applicationstudio.cloud.sap/login/callback"
        ]
    },
    "scopes": [
        {
            "name": "uaa.user",
            "description": "UAA"
        }
    ],
    "role-templates": [
        {
            "name": "Token_Exchange",
            "description": "UAA",
            "scope-references": [
                "uaa.user"
            ]
        }
    ]
}