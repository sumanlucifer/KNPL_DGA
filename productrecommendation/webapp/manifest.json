{
    "_version": "1.32.0",
    "sap.app": {
        "id": "com.knpl.dga.productrecommendation",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "resources": "resources.json",
        "dataSources": {
            "mainService": {
                "uri": "/api/v2/odata.svc/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "odataVersion": "2.0",
                    "localUri": "localService/metadata.xml"
                }
            }
        },
        "crossNavigation": {
            "inbounds": {
                "com-knpl-dga-productrecommendation-inbound": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "ProRecommendation",
                    "action": "Manage",
                    "title": "{{flpTitle}}",
                    "subTitle": "{{flpSubtitle}}",
                    "icon": "sap-icon://activity-individual",
                    "indicatorDataSource": {
                        "dataSource": "KNPL_DGA_API",
                        "path": "/dynamic_dest/KNPL_DGA_API/api/v2/odata.svc/ProductRecommendations/$count?$filter=IsArchived eq false",
                        "refresh": 0
                    }
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://task",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "dependencies": {
            "minUI5Version": "1.98.0",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.f": {},
                "sap.suite.ui.generic.template": {},
                "sap.ui.comp": {},
                "sap.ui.generic.app": {},
                "sap.ui.table": {},
                "sap.ushell": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.knpl.dga.productrecommendation.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false,
                    "defaultUpdateMethod": "Put",
                    "useBatch": false
                }
            },
            "LoginInfo": {
                "type": "sap.ui.model.json.JSONModel"
            }
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "com.knpl.dga.productrecommendation.view",
                "controlId": "app",
                "controlAggregation": "pages",
                "bypassed": {
                    "target": [
                        "notFound"
                    ]
                },
                "async": true
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "worklist",
                    "target": [
                        "worklist"
                    ]
                },
                {
                    "pattern": "Products/{objectId}",
                    "name": "object",
                    "target": [
                        "object"
                    ]
                },
                {
                    "name": "ActionPage",
                    "pattern": "ActionPage/{action}/:property:",
                    "target": [
                        "TargetActionPage"
                    ]
                },
                {
                    "name": "DGAList",
                    "pattern": "DGAList/{catalogueId}",
                    "target": [
                        "TargetPainterList"
                    ]
                }
            ],
            "targets": {
                "worklist": {
                    "viewName": "Worklist",
                    "viewId": "worklist",
                    "viewLevel": 1,
                    "title": "{i18n>worklistViewTitle}"
                },
                "object": {
                    "viewName": "Object",
                    "viewId": "object",
                    "viewLevel": 2,
                    "title": "{i18n>objectViewTitle}"
                },
                "objectNotFound": {
                    "viewName": "ObjectNotFound",
                    "viewId": "objectNotFound"
                },
                "notFound": {
                    "viewName": "NotFound",
                    "viewId": "notFound"
                },
                "TargetActionPage": {
                    "viewType": "XML",
                    "transition": "slide",
                    "viewName": "ActionPage",
                    "viewLevel": 2,
                    "viewId": "actionPage"
                },
                "TargetPainterList": {
                    "viewType": "XML",
                    "transition": "slide",
                    "viewName": "PainterList",
                    "viewLevel": 2,
                    "viewId": "painterList"
                }
            }
        },
        "rootView": {
            "viewName": "com.knpl.dga.productrecommendation.view.App",
            "type": "XML",
            "async": true,
            "id": "app"
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "knpl.dga"
    }
}